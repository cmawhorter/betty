{
  "name": "bty",
  "version": "2.0.0",
  "description": "Simple command line utility for developing and deploying AWS Lambda functions.",
  "main": "./lib/main.js",
  "bin": {
    "bty": "./cli/main.js",
    "betty": "./cli/main.js"
  },
  "scripts": {
    "lintcli": "./node_modules/.bin/eslint ./cli",
    "lintlib": "./node_modules/.bin/eslint ./lib",
    "linttests": "./node_modules/.bin/eslint ./test",
    "lint": "npm run lintcli && npm run lintlib && npm run linttests",
    "pretest": "npm run lint",
    "test": "./node_modules/.bin/mocha -r ./test/lib/helper test",
    "coverage": "./node_modules/.bin/nyc npm run test"
  },
  "author": {
    "name": "Cory Mawhorter",
    "email": "cory.mawhorter@gmail.com",
    "url": "http://www.mawhorter.net/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cmawhorter/betty.git"
  },
  "bugs": {
    "url": "https://github.com/cmawhorter/betty/issues"
  },
  "license": "MIT",
  "dependencies": {
    "ajv": "4.x",
    "archiver": "1.x",
    "builtin-modules": "^3.1.0",
    "dotenv": "^8.2.0",
    "fs-extra": "^9.0.0",
    "inquirer": "^7.1.0",
    "jsonc-parser": "^2.2.1",
    "lodash": "^4.17.15",
    "reify": "^0.20.12",
    "stream-buffers": "3.x",
    "urn-lib": "1.x",
    "yargs": "15.x"
  },
  "peerDependencies": {
    "aws-sdk": "2.x"
  },
  "devDependencies": {
    "aws-sdk": "2.x",
    "chai": "*",
    "eslint": "*",
    "mocha": "*",
    "nyc": "*"
  },
  "files": [
    "lib/",
    "cli/"
  ],
  "keywords": [
    "aws",
    "lambda",
    "microservice"
  ]
}
