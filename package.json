{
  "name": "bty",
  "version": "2.0.0",
  "description": "Simple command line utility for developing and deploying AWS Lambda functions.",
  "main": "./lib/main.js",
  "bin": {
    "bty": "./cli/main.js",
    "betty": "./cli/main.js"
  },
  "scripts": {
    "preinstall": "node -e 'if(/yarn\\.js$/.test(process.env.npm_execpath))throw new Error(\"This repo requires npm\")'",
    "lintcli": "./node_modules/.bin/eslint ./cli",
    "lintcommon": "./node_modules/.bin/eslint ./common",
    "linttests": "./node_modules/.bin/eslint ./test",
    "lint": "npm run lintcli && npm run lintcommon && npm run linttests",
    "pretest": "npm run lint",
    "test": "./node_modules/.bin/mocha -r ./test/lib/helper test",
    "coverage": "./node_modules/.bin/nyc npm run test"
  },
  "author": {
    "name": "Cory Mawhorter",
    "email": "cory.mawhorter@gmail.com",
    "url": "http://www.mawhorter.net/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cmawhorter/betty.git"
  },
  "bugs": {
    "url": "https://github.com/cmawhorter/betty/issues"
  },
  "license": "MIT",
  "dependencies": {
    "ajv": "4.x",
    "archiver": "1.x",
    "esm": "^3.2.25",
    "jsonc-parser": "^2.2.1",
    "mkdirp": "1.x",
    "rimraf": "2.x",
    "slug": "3.x",
    "stream-buffers": "3.x",
    "urn-lib": "1.x",
    "yargs": "15.x"
  },
  "peerDependencies": {
    "aws-sdk": "2.x"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.0.3",
    "aws-sdk": "2.x",
    "chai": "*",
    "eslint": "*",
    "mocha": "*",
    "nyc": "*"
  },
  "keywords": [
    "aws",
    "lambda",
    "microservice"
  ]
}
